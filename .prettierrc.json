{
    "semi": true,
    "quoteProps": "as-needed",
    "printWidth": 100,
    "tabWidth": 4,
    "overrides": [
        {
            "files": "*.ejs",
            "options": {
                "parser": "html"
            }
        },
        {
            "files": "*.json",
            "options": {
                "parser": "json"
            }
        }
    ]
}
